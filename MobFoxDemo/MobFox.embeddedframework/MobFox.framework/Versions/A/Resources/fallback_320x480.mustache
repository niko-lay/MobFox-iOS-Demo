	    {{{libs}}}
	</head>
        <body data-rating="{{textassets.rating}}">
            <a id="clickLink" href="{{{click_url}}}" target="_top">
                <div class="bg bg-color"></div>
                <div class="title title-position">
                    {{textassets.headline}}
                </div>
                <div class="stars">
                    <div class="star"></div>
                    <div class="star"></div>
                    <div class="star"></div>
                    <div class="star"></div>
                    <div class="star"></div>
                </div>
                <div class="btn btn-style btn-txt">Install</div>

            </a>
            <script>!function(){var e,t=document.querySelectorAll(".star"),o=document.querySelector(".stars");if(e=Math.ceil(parseFloat(document.body.dataset.rating)),e>3)for(var a=0;e>a;a++)t[a].style.backgroundPosition="top left";else o.style.display="none"}();</script>
            <script>function init(){renderer=Detector.webgl?new THREE.WebGLRenderer({alpha:!0}):new THREE.CanvasRenderer({alpha:!0}),renderer.setPixelRatio(window.devicePixelRatio),renderer.setSize(320,320),renderer.domElement.style.pointerEvents="none",document.body.appendChild(renderer.domElement),camera=new THREE.PerspectiveCamera(50,1,1,1e3),camera.position.z=400,scene=new THREE.Scene;var e=[new THREE.MeshBasicMaterial({color:16711680,transparent:!0,opacity:.5,side:THREE.BackSide}),new THREE.MeshBasicMaterial({color:16776960,transparent:!0,opacity:.5,side:THREE.BackSide}),new THREE.MeshBasicMaterial({color:65280,transparent:!0,opacity:.5,side:THREE.DoubleSide}),new THREE.MeshBasicMaterial({color:65535,transparent:!0,opacity:.5,side:THREE.DoubleSide}),new THREE.MeshBasicMaterial({color:255,transparent:!0,opacity:.5,side:THREE.DoubleSide}),new THREE.MeshBasicMaterial({color:16777215,transparent:!0,opacity:.5,side:THREE.DoubleSide})];e.anisotropy=renderer.getMaxAnisotropy(),mesh=new THREE.Mesh(new THREE.BoxGeometry(225,225,225),new THREE.MeshFaceMaterial(e)),scene.add(mesh);var a=new THREE.PlaneBufferGeometry(200,200,50),t=new THREE.MeshBasicMaterial({map:THREE.ImageUtils.loadTexture("{{{imageassets.icon.url}}}"),side:THREE.DoubleSide});t.minFilter=t.magFilter=THREE.LinearFilter,plane=new THREE.Mesh(a,t),scene.add(plane)}function animate(){requestAnimationFrame(animate),mesh.rotation.y+=.015,Math.abs(plane.rotation.y)>=.25?(rotStep*=-1,plane.rotation.y+=rotStep,plane.rotation.x+=rotStep):(plane.rotation.y+=rotStep,plane.rotation.x+=rotStep),Math.abs(plane.position.x)>=15?(posStep*=-1,plane.position.x+=posStep,plane.position.y+=posStep):(plane.position.x+=posStep,plane.position.y+=posStep),renderer.render(scene,camera)}var rotStep=.002,posStep=.05;init(),animate();</script>
